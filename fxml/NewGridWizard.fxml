<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import net.dragondelve.customdriversutil.fx.css.PromptLabel?>
<VBox fx:id="rootPane" prefHeight="475.0" prefWidth="784.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1">
    <PromptLabel alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="35.0" prefWidth="602.0"
                 text="New Grid Wizard" textAlignment="CENTER"/>
    <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
        <opaqueInsets>
            <Insets/>
        </opaqueInsets>
        <VBox prefHeight="380.0" prefWidth="117.0" spacing="10.0">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
            <CheckBox fx:id="emptyGridCheckBox" mnemonicParsing="false" text="Empty Grid"/>
            <CheckBox fx:id="generateGridCheckBox" mnemonicParsing="false" text="Generate Grid"/>
        </VBox>
        <Separator orientation="VERTICAL" prefHeight="200.0"/>
        <GridPane hgap="10.0" prefHeight="400.0" prefWidth="595.0" vgap="10.0" HBox.hgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="232.0" minWidth="10.0" prefWidth="200.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="313.0" minWidth="10.0" prefWidth="200.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="313.0" minWidth="10.0" prefWidth="200.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="313.0" minWidth="10.0" prefWidth="200.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
            <ChoiceBox fx:id="vehicleClassChoiceBox" maxWidth="1.7976931348623157E308" prefHeight="25.0"
                       prefWidth="147.0" GridPane.columnIndex="1"/>
            <PromptLabel text="Choose Vehicle Class"/>
            <CheckBox fx:id="randomValuesCheckBox" mnemonicParsing="false" text="Random Values"
                      GridPane.rowIndex="1"/>
            <CheckBox fx:id="rangeOfValuesCheckBox" mnemonicParsing="false" text="Range of Values"
                      GridPane.rowIndex="2"/>
            <RadioButton fx:id="randomAllRadioButton" mnemonicParsing="false" text="Random ALL"
                         GridPane.columnIndex="1" GridPane.rowIndex="1"/>
            <RadioButton fx:id="randomSkillRadioButton" mnemonicParsing="false" text="Random Skill"
                         GridPane.columnIndex="2" GridPane.rowIndex="1"/>
            <RadioButton fx:id="useNAMeSRadioButton" mnemonicParsing="false" text="Use NAMeS"
                         GridPane.columnIndex="1" GridPane.rowIndex="6"/>
            <RadioButton fx:id="blankNamesRadioButton" mnemonicParsing="false" text="Blank Names"
                         GridPane.rowIndex="6"/>
            <RadioButton fx:id="fromLiveryNamesRadioButton" mnemonicParsing="false" text="From Livery Name"
                         GridPane.columnIndex="3" GridPane.rowIndex="6"/>
            <RadioButton fx:id="useNAMeSPlusRadioButton" mnemonicParsing="false" text="Use NAMeS+"
                         GridPane.columnIndex="2" GridPane.rowIndex="6"/>
            <RadioButton fx:id="noNoizeRadioButton" mnemonicParsing="false" text="No Noize"
                         GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            <RadioButton fx:id="lowNoizeRadioButton" mnemonicParsing="false" text="Low Noize"
                         GridPane.columnIndex="2" GridPane.rowIndex="2"/>
            <RadioButton fx:id="highNoizeRadioButton" mnemonicParsing="false" text="High Noize"
                         GridPane.columnIndex="3" GridPane.rowIndex="2"/>
            <CheckBox fx:id="bindQualiCheckBox" mnemonicParsing="false" text="Bind Quali &amp; Race Skills"
                      GridPane.rowIndex="5"/>
            <CheckBox fx:id="qualiExceedsRaceSkillCheckBox" mnemonicParsing="false"
                      text="Quali Skill &gt; Race Skill" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            <CheckBox fx:id="noValuesCheckBox" mnemonicParsing="false" text="No Values" GridPane.rowIndex="3"/>
            <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="2" GridPane.rowIndex="5">
                <PromptLabel alignment="CENTER_RIGHT" prefHeight="45.0" prefWidth="187.0" text="By This Amount:"/>
                <TextField fx:id="qualiExceedsTextField" GridPane.columnIndex="3" GridPane.rowIndex="5"/>
            </HBox>
            <HBox alignment="CENTER" GridPane.rowIndex="4">
                <PromptLabel prefHeight="30.0" prefWidth="130.0" text="Min Value"/>
                <TextField fx:id="minValueTextField" GridPane.columnIndex="1" GridPane.rowIndex="4"
                           HBox.hgrow="ALWAYS"/>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                <PromptLabel prefHeight="17.0" prefWidth="140.0" text="Max Value"/>
                <TextField fx:id="maxValueTextField" GridPane.columnIndex="3" GridPane.rowIndex="4"
                           HBox.hgrow="ALWAYS"/>
            </HBox>
            <CheckBox mnemonicParsing="false" text="Limit Aggression" GridPane.columnIndex="2"
                      GridPane.rowIndex="4"/>
            <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
                <PromptLabel text="Limit to: "/>
                <TextField fx:id="limitToTextField" prefHeight="25.0" prefWidth="79.0"
                           GridPane.columnIndex="3" HBox.hgrow="ALWAYS"/>
            </HBox>
            <CheckBox fx:id="forEachLiveryCheckBox" mnemonicParsing="false" text="For Each Livery"
                      GridPane.columnIndex="2"/>
            <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="3">
                <PromptLabel prefHeight="17.0" prefWidth="118.0" text="Amount"/>
                <TextField fx:id="amountCheckBox" GridPane.columnIndex="3" HBox.hgrow="ALWAYS"/>
            </HBox>
            <CheckBox fx:id="reduceGapOnOvalsCheckBox" mnemonicParsing="false" text="Reduce gap on Ovals"
                      GridPane.columnIndex="3" GridPane.rowIndex="3"/>
        </GridPane>
    </HBox>
    <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0">
        <Button fx:id="generateButton" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Generate"/>
    </ToolBar>
</VBox>
